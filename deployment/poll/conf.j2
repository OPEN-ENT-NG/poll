{
    "name": "{{ pollVersion }}",
    "config": {
        "main" : "net.atos.entng.poll.Poll",
        "port" : 8070,
        {% if csrfToken %}"csrf-token": true,{% endif %}
        "app-name" : "Poll",
        "app-address" : "/poll",
        "app-icon" : "poll-large",
        "app-type" : "END_USER",
        "auto-redeploy": false,
        {% if overrideTheme %} "override-theme": {{ override2d }}, {% endif %}
        "host": "https://{{ host }}",
        "ssl" : true,
        "userbook-host": "https://{{ host }}",
        "integration-mode" : "HTTP",
        "app-registry.port" : 8012,
        "mode" : "prod",
        "entcore.port" : 8009,
        "publicConf": {
            "xiti": {
                "ID_SERVICE": {
                    "default": 2
                },
                "LIBELLE_SERVICE": {
                    "default": "PRODUCTION_COLLABORATIVE"
                }
            }
        }
    }
}